@{
    ViewBag.Title = "Randevu İptal";
}

<div class="col-md-12">
    <div class="box box-solid">
        <div class="box-header with-border">
            <i class="fa fa-times-circle"></i>

            <h3 class="box-title">Hatalı Giriş</h3>
        </div>
        <!-- /.box-header -->
        <div class="box-body">
            <p>Girdiğiniz kod yanlıştır.</p>
            <p>@ViewBag.Doctor adlı doktora alınan @ViewBag.Date tarihli randevu silinmiştir.</p>
            @Html.ActionLink("Yeni Randevu Al", "GetOffice", "Appointment", null, new { @class = "btn btn-block btn-warning" })
        </div>
        <!-- /.box-body -->
    </div>
    <!-- /.box -->
</div>


